{
  "name": "scrumy-hp_frontend",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "run-p -c lint:*",
    "lint:eslint": "eslint src --ext .js,jsx,.ts,.tsx",
    "lint:prettier": "prettier --check .",
    "fix": "run-s -c fix:eslint fix:prettier",
    "fix:eslint": "eslint src --ext .js,jsx,.ts,.tsx --fix",
    "fix:prettier": "prettier --write .",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "deploy:gae": "yarn build && gcloud app deploy app.yaml --project=scrumy-hp-test"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn fix:eslint",
      "yarn fix:prettier"
    ],
    "*.{json,md,mdx,yml,yaml,css,scss}": [
      "yarn fix:prettier"
    ]
  },
  "dependencies": {
    "@chakra-ui/react": "^2.0.0",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "framer-motion": "^6",
    "next": "12.1.6",
    "react": "18.1.0",
    "react-dom": "18.1.0"
  },
  "devDependencies": {
    "@types/node": "17.0.33",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.4",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "clsx": "^1.1.1",
    "eslint": "8.15.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "typescript": "4.6.4"
  }
}
